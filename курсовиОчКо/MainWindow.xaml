        <TextBlock HorizontalAlignment="Left" Height="21" Margin="503,54,0,0" TextWrapping="Wrap" Text="*грусть*" VerticalAlignment="Top" Width="81" FontFamily="Agency FB"/>
        <TextBlock HorizontalAlignment="Left" Height="21" Margin="503,106,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="81" FontFamily="Agency FB"><Run Text="*"/><Run Text="люблю*"/></TextBlock>
